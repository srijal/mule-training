	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="bf558f72-9b2f-4a57-ade3-6e5c7f222dd4" >
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
	</http:listener-config>
	<db:config name="Database_Config" doc:name="Database Config" doc:id="d8d949b7-3c0e-4e52-a6d4-79d4bb2e4424" >
		<db:mssql-connection host="${mssql.host}" port="${mssql.port}" user="${mssql.user}" password="${mssql.password}" databaseName="${mssql.dbname}" />
	</db:config>
	<amqp:config name="AMQP_Config" doc:name="AMQP Config" doc:id="fc7296c7-da0e-4b2b-bc02-4dc3808964cc">
		<amqp:connection host="${rabbitmq.host}" port="${rabbitmq.port}" virtualHost="${rabbitmq.vhost}" username="${rabbitmq.user}" password="${rabbitmq.password}" />
	</amqp:config>
	<http:request-config name="HTTP_Request_configuration_rabbitmq" doc:name="HTTP Request configuration" doc:id="c44666f9-b117-49fc-981c-2684911f7545" basePath="/api" >
		<http:request-connection host="${rabbitmq.host}" port="${rabbitmq.port}" >
			<http:authentication >
				<http:basic-authentication username="${rabbitmq.user}" password="${rabbitmq.password}" />
			</http:authentication>
		</http:request-connection>
