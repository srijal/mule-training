	exchangename: payload01.ExchangeName,
	exchangetype: payload01.ExchangeType,
	queuename: payload01.QueueName default "",
	routingkey: payload01.RoutingKey,
	headers: payload01.Headers
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
