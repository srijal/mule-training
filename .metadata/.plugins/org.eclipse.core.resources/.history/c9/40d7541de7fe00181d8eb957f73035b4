	<amqp:config name="AMQP_Config" doc:name="AMQP Config" doc:id="f3b732a8-5180-492a-8c36-88246274c341">
		<amqp:connection host="${rabbitmq.host}" port="${rabbitmq.port}" virtualHost="${rabbitmq.vhost}" username="${rabbitmq.user}" password="${rabbitmq.password}" />
	</amqp:config>
	<http:request-config name="HTTP_Request_configuration_rabbitmq" doc:name="HTTP Request configuration" doc:id="fa8a9fca-32d8-4a9e-ba1c-d733b2d85415" basePath="/api" >
		<http:request-connection host="${rabbitmq.host}" port="${rabbitmq.port}" >
			<http:authentication >
				<http:basic-authentication username="${rabbitmq.user}" password="${rabbitmq.password}" />
