    <properties>
        <licensePath>LICENSE.md</licensePath>
        <api.version>43.0.0</api.version>
        <sonar.coverage.exclusions>**org/mule/extension/salesforce/api/utility/AdditionalInformationMap.java,**org/mule/extension/salesforce/internal/service/dto/core/AdditionalInformationMapDTO.java,**org/mule/extension/salesforce/api/error/ApexOperationErrorTypeProvider.java,**org/mule/extension/salesforce/internal/service/exception/AsyncRequestSentButNotFinished.java,**org/mule/extension/salesforce/internal/service/dto/core/DifferenceTypeDTO.java,**org/mule/extension/salesforce/internal/service/soap/EnrichedUpsertResult.java,**org/mule/extension/salesforce/api/utility/FieldDiff.java,**org/mule/extension/salesforce/internal/service/dto/core/FieldDiffDTO.java,**org/mule/extension/salesforce/internal/service/antlr/apex/JavaBaseListener.java,**org/mule/extension/salesforce/internal/source/ModifiedObject.java,**org/mule/extension/salesforce/internal/source/NewObject.java,**org/mule/extension/salesforce/api/param/OAuth2Params.java,**org/mule/extension/salesforce/api/param/bundle/OAuth2ParamBundle.java,**org/mule/extension/salesforce/internal/service/streaming/generic/PushEventResult.java,**org/mule/extension/salesforce/api/utility/ResetPasswordResult.java,**org/mule/extension/salesforce/internal/service/connection/bayeux/SalesforceBayeuxConstants.java,**org/mule/extension/salesforce/internal/service/dto/search/SearchDefinitionDTO.java,**org/mule/extension/salesforce/api/search/SearchResult.java,**org/mule/extension/salesforce/internal/service/dto/search/SearchResultDTO.java,**org/mule/extension/salesforce/internal/service/antlr/soslquery/generated/SoslBaseListener.java,**org/mule/extension/salesforce/internal/service/antlr/soslquery/generated/SoslBaseVisitor.java,**org/mule/extension/salesforce/internal/service/antlr/soslquery/generated/SoslLexer.java,**org/mule/extension/salesforce/internal/service/antlr/soslquery/generated/SoslParser.java,**org/mule/extension/salesforce/internal/service/antlr/nativequery/generated/SoqlParser.java,**org/mule/extension/salesforce/internal/service/antlr/nativequery/generated/SoqlLexer.java,**org/mule/extension/salesforce/internal/service/antlr/nativequery/generated/SoqlBaseListener.java,**org/mule/extension/salesforce/internal/service/antlr/apex/JavaBaseVisitor.java, **org/mule/extension/salesforce/internal/service/antlr/apex/JavaParser.java,**org/mule/extension/salesforce/internal/service/antlr/apex/JavaLexer.java</sonar.coverage.exclusions>
    </properties>

    <build>
        <plugins>
            <!-- Prevent maven from filtering files with specific extension because
                maven is corrupting them during filter -->
            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <version>2.5</version>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
                    <nonFilteredFileExtensions>
                        <nonFilteredFileExtension>jks</nonFilteredFileExtension>
                        <nonFilteredFileExtension>zip</nonFilteredFileExtension>
                    </nonFilteredFileExtensions>
                </configuration>
            </plugin>

            <!-- Salesforce is the only connector that is Select and it's published
                in a public repository. This is the reason why the sourcecode attachment
                must be disabled -->
            <plugin>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.1.2</version>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <phase>none</phase>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.antlr</groupId>
                <artifactId>antlr4-maven-plugin</artifactId>
                <version>4.5.1</version>
                <configuration>
                    <sourceDirectory>src/main/resources</sourceDirectory>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>3.0.2</version>
                <executions>
                    <execution>
                        <id>connector-analyze</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>analyze-only</goal>
                        </goals>
                        <configuration>
                            <ignoredDependencies>
                                <ignoredDependency>org.mule.*</ignoredDependency>
                                <ignoredDependency>org.eclipse.jetty</ignoredDependency>
                                <ignoredDependency>org.bouncycastle:bcprov-jdk15on</ignoredDependency>
                                <ignoredDependency>com.sun.xml.ws:jaxws-rt</ignoredDependency>
                            </ignoredDependencies>
                            <failOnWarning>true</failOnWarning>
                            <ignoredUsedUndeclaredDependencies>
                                <ignoredUsedUndeclaredDependency>*</ignoredUsedUndeclaredDependency>
                            </ignoredUsedUndeclaredDependencies>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

